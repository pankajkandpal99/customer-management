{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/cron/overdue",
      "dest": "/api/cron/overdue"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "continue": true
    }
  ],
  "crons": [
    {
      "path": "/api/cron/overdue",
      "schedule": "0 0 * * *"
    }
  ]
}
